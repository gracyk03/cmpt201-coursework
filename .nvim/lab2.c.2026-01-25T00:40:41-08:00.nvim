#include <stdio.h>
#include <unistd.h>
#include <string.h>
#include <stdlib.h>

int main() { 
  char *line = NULL;
  size_t *len = 0;
  ssize_t nread;

  printf("Enter programs to run: \n"); 
  nread = getline(&line, &len, stdin);

  if(nread == -1){
    perror("error: getline failed");
    free(line);
    exit(EXIT_FAILURE);
  }

  pid_t pid = fork();
}
